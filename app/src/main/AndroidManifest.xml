<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MaterialComponents.Light"
        tools:targetApi="31">
        <activity
            android:name=".AddLaptopActivity"
            android:exported="false"
            android:label="Add Laptop Data"/>
        <activity
            android:name=".LaptopActivity"
            android:exported="false"
            android:label="List of Laptop Data"/>
        <activity
            android:name=".UpdateLaptopBrandActivity"
            android:exported="false"
            android:label="Update Laptop Brand Data"/>
        <activity
            android:name=".AddLaptopBrandActivity"
            android:exported="false"
            android:label="Add Laptop Brand Data"
            android:parentActivityName=".LaptopBrandActivity" />
        <activity
            android:name=".LaptopBrandActivity"
            android:exported="false"
            android:label="List of Laptop Brand Data"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="Laptop Management">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>